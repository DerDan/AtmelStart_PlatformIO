{
  "build": {
    "cpu": "cortex-m0plus",
    "extra_flags": "-DARDUINO_SAMD_ZERO -DARDUINO_ARCH_SAMD -D__SAMD21G18A__",
    "f_cpu": "48000000L",
    "mcu": "samd21g18a"
  },
  "debug": {
    "jlink_device": "ATSAMD21G18",
    "openocd_chipname": "at91samd21g18",
    "openocd_target": "at91samdXX",
    "svd_path": "ATSAMD21G18A.svd"
  },
  "frameworks": [],
  "name": "Arduino Zero ASF",
  "upload": {
    "disable_flushing": true,
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "protocol": "jlink",
    "protocols": [
      "sam-ba",
      "blackmagic",
      "jlink",
      "atmel-ice"
    ]
  },
  "url": "https://www.arduino.cc/en/Main/ArduinoBoardZero",
  "vendor": "Arduino"
}